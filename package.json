{
  "name": "ai-chat-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "AI Chat Application",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev:api": "pnpm --filter @prototype/api dev",
    "dev:admin": "pnpm --filter @prototype/admin dev",
    "build:api": "pnpm --filter @prototype/shared build && pnpm --filter @prototype/api build",
    "build:admin": "pnpm --filter @prototype/shared build && pnpm --filter @prototype/admin build",
    "build:all": "pnpm --filter @prototype/shared build && pnpm --filter @prototype/api build && pnpm --filter @prototype/admin build"
  },
  "devDependencies": {
    "prettier": "^3.2.5",
    "supabase": "^2.34.3"
  }
}