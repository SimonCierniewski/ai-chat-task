{
  "name": "@prototype/shared",
  "version": "0.0.1",
  "private": true,
  "description": "Shared utilities, types, and business logic",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./api/chat": {
      "types": "./dist/api/chat.d.ts",
      "require": "./dist/api/chat.js"
    },
    "./api/memory": {
      "types": "./dist/api/memory.d.ts",
      "require": "./dist/api/memory.js"
    },
    "./api/admin": {
      "types": "./dist/api/admin.d.ts",
      "require": "./dist/api/admin.js"
    },
    "./api": {
      "types": "./dist/api/index.d.ts",
      "require": "./dist/api/index.js"
    },
    "./telemetry": {
      "types": "./dist/telemetry.d.ts",
      "require": "./dist/telemetry.js"
    },
    "./telemetry-memory": {
      "types": "./dist/telemetry-memory.d.ts",
      "require": "./dist/telemetry-memory.js"
    },
    "./memory-config": {
      "types": "./dist/memory-config.d.ts",
      "require": "./dist/memory-config.js"
    },
    "./pricing": {
      "types": "./dist/pricing.d.ts",
      "require": "./dist/pricing.js"
    },
    "./graph": {
      "types": "./dist/graph.d.ts",
      "require": "./dist/graph.js"
    },
    "./aggregates": {
      "types": "./dist/aggregates.d.ts",
      "require": "./dist/aggregates.js"
    },
    "./admin-settings": {
      "types": "./dist/admin-settings.d.ts",
      "require": "./dist/admin-settings.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "ajv": "^8.12.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}
