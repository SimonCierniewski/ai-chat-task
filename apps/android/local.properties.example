# =======================================
# Android App Environment Variables
# =======================================
# Copy to local.properties and fill with actual values
# NEVER commit local.properties to version control

# ===================================
# Development Environment Configuration
# ===================================

# Supabase Configuration (Dev)
dev.supabase.url=https://xxxxxxxxxxxxxxxxxxxx.supabase.co
dev.supabase.anonKey=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.xxxxxxxxxxxx

# API Configuration (Dev)
# Use 10.0.2.2 for Android emulator to access host machine's localhost
# Use your machine's IP address (e.g., 192.168.1.100) for physical device testing
dev.api.baseUrl=http://10.0.2.2:3000

# Deep Link Configuration (Dev)
dev.app.deeplink.scheme=aichat-dev
dev.app.deeplink.host=auth

# ===================================
# Production Environment Configuration
# ===================================

# Supabase Configuration (Prod)
prod.supabase.url=https://xxxxxxxxxxxxxxxxxxxx.supabase.co
prod.supabase.anonKey=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.xxxxxxxxxxxx

# API Configuration (Prod)
prod.api.baseUrl=https://api.example.com

# Deep Link Configuration (Prod)
prod.app.deeplink.scheme=aichat
prod.app.deeplink.host=auth

# ===================================
# Optional: Feature Flags
# ===================================
# You can add feature flags that differ between dev/prod
# feature.enable_analytics=false
# feature.enable_crash_reporting=false
